{
    "directories": {
		"output_directory": "./",
		"logging_directory": "./logs/"
    },
    "execution_control": {
        "allocation": "pxs",
        "nodes": 10,
        "option": "eagle",
		"memory": 90,
		"walltime": 4.0,
		"feature": "--qos=high",
        "sites_per_core": 100
    },
    "project_control": {
		"analysis_years": [2012],
        "logging_level": "INFO",
		"output_request": [
		  "cf_mean",
		  "cf_profile",
		  "lcoe_fcr"
		],
        "technology": "pv"
    },
    "project_points": "./project_points.csv",
    "resource_file": "/datasets/NSRDB/v3.0.1/nsrdb_{}.h5",
    "sam_files": {
		"default": "./rpm_pv.json"
	}
}
